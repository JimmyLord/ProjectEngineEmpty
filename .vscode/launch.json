{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Launch",
            "type": "cppdbg",
            "request": "launch",
            "osx": {
                "program": "${workspaceRoot}/GameEmptyReplaceMe/Output/EmptyReplaceMe/Build/Products/Debug/EmptyReplaceMe-OSX.app/Contents/MacOS/EmptyReplaceMe-OSX",
                "MIMode": "lldb"
            },
            "linux": {
                "program": "${workspaceRoot}/GameEmptyReplaceMe/build/Debug/ProjectEmptyReplaceMe",
                "MIMode": "gdb"
            },
            "environment": [],
            "cwd": "${workspaceRoot}/GameEmptyReplaceMe/Game",
            "args": [],
            "stopAtEntry": false,
            "externalConsole": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "wxDebug Launch",
            "type": "cppdbg",
            "request": "launch",
            "osx": {
                "program": "${workspaceRoot}/GameEmptyReplaceMe/Output/EmptyReplaceMe/Build/Products/Debug/EmptyReplaceMe-wx.app/Contents/MacOS/EmptyReplaceMe-wx",
                "MIMode": "lldb",
                "environment": []
            },
            "linux": {
                "program": "${workspaceRoot}/GameEmptyReplaceMe/build/wxDebug/ProjectEmptyReplaceMe",
                "MIMode": "gdb",
                "environment": [{"name":"GDK_SYNCHRONIZE", "value":"1"}]
            },
            "cwd": "${workspaceRoot}/GameEmptyReplaceMe/Game",
            "args": [],
            "stopAtEntry": false,
            "externalConsole": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}